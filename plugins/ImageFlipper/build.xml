<project name="ImageFlipper" default="jar">
	<property name="plugin.path" value="On-The-Fly_Processors"/>
    <import file="../javapluginbuild.xml"/>

    <target name="copy-files" depends="mm.javabuild.copy-files">
        <copy todir="${intdir}">
            <fileset dir="${rscdir}" includes="**/*.png"/>
        </copy>
    </target>
</project>
